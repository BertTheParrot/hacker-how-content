{
  "parser": "babel-eslint",
  "extends": ["@jokeyrhyme/es2015", "import/warnings"],
  "ecmaFeatures": {
    "jsx": true
  },

  "env": {
  },

  "globals": {},

  "plugins": [
    "import",
  ],

  "rules": {
    comma-dangle: [0, "never"],
    import/no-unresolved: [0],
    import/no-require: [2],
    no-console: [0],
    semi: [2, "never"],
    space-before-function-paren: [2, "never"],
    space-return-throw-case: [0],
    object-shorthand: [0],
    complexity: [0],
    no-process-env: [0]
  },
  "settings": {
    import/parser: "babel-eslint"
  }
}
